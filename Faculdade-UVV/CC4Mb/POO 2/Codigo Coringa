#include <stdio.h>
void imprime(int *m, int n){
    int i;
    for (i=0; i < n; i++){
        printf ("%d \n", m[i]);
    }
}

void multi(int vet[],int x){
    for (int i = 0; i < 5; i++) {
        int j = vet[i] * x;
        printf("%d\n", j);
    }
}

int main()
{
    int x;
    int vet[5] = {1,2,3,4,5};
    printf("Escreve um numero: ");
    scanf("%d", &x);
    multi(vet, x);
    
    return 0;
}
